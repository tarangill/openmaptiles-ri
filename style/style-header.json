{
  "version": 8,
  "name": "OSM OpenMapTiles",
  "id": "openmaptiles",
  "center": [
    0,
    0
  ],
  "zoom": 1,
  "bearing": 0,
  "pitch": 0,
  "sources": {
    "openmaptiles": {
      "type": "vector",
      "tiles": ["https://maps.tarangill.com/tiles/{z}/{x}/{y}.pbf"]
    },
    "terrarium": {
  	"type": "raster-dem",
  	"tiles": ["https://maps.tarangill.com/terrain/{z}/{x}/{y}.png"],
  	"minzoom": 0,
  	"maxzoom": 10,
  	"tileSize": 256,
  	"encoding": "terrarium"
    }
  },
  "glyphs": "{fontstack}/{range}.pbf",
  "sprite": "sprite",
  "layers": [
	  
    {
      "id": "background",
      "type": "background",
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "background-color": "#EFEFEF"
      }
    }
  ]
}
